{% extends '::layout.html.twig' %}

{% block content %}
    <h1>Asin</h1>

    <table>
        <tbody>
            <tr>
                <th>Asin</th>
                <td>{{ asin.asin }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ asin.title }}</td>
            </tr>
            <tr>
                <th>Map</th>
                <td>{{ asin.map }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ asin.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('asin_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('asin_edit', { 'id': asin.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
