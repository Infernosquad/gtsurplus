<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="asinchild_index" path="/" methods="GET">
        <default key="_controller">AppBundle:AsinChild:index</default>
    </route>

    <route id="asinchild_show" path="/{id}/show" methods="GET">
        <default key="_controller">AppBundle:AsinChild:show</default>
    </route>

    <route id="asinchild_new" path="/new" methods="GET|POST">
        <default key="_controller">AppBundle:AsinChild:new</default>
    </route>

    <route id="asinchild_edit" path="/{id}/edit" methods="GET|POST">
        <default key="_controller">AppBundle:AsinChild:edit</default>
    </route>

    <route id="asinchild_delete" path="/{id}/delete" methods="DELETE">
        <default key="_controller">AppBundle:AsinChild:delete</default>
    </route>

</routes>
